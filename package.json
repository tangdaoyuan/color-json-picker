{
  "name": "@suger-tdy/color-json-picker",
  "version": "0.0.1",
  "packageManager": "pnpm@7.1.9",
  "description": "pick colors from json file",
  "keywords": [
    "json picker",
    "color picker"
  ],
  "license": "MIT",
  "author": "tangdaoyuan <1197633750@qq.com> (https://github.com/tangdaoyuan/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangdaoyuan/color-json-picker.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "bin": {
    "c-picker": "./bin/c-picker.js"
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsx ./src/index.ts",
    "stub": "unbuild --stub",
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --tag --commit --push && pnpm publish"
  },
  "bugs": {
    "url": "https://github.com/tangdaoyuan/color-json-picker/issues"
  },
  "homepage": "https://github.com/tangdaoyuan/color-json-picker#readme",
  "dependencies": {
    "cli-progress": "^3.11.1",
    "debug": "^4.3.4",
    "picocolors": "^1.0.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@suger-tdy/eslint-config": "1.1.3",
    "@types/cli-progress": "^3.11.0",
    "@types/debug": "^4.1.7",
    "@types/yargs": "^17.0.10",
    "bumpp": "7.1.1",
    "esbuild": "0.14.42",
    "eslint": "8.17.0",
    "tsx": "3.4.2",
    "typescript": "4.7.3",
    "unbuild": "0.7.4",
    "vite": "2.9.9",
    "vitest": "0.14.1"
  }
}
